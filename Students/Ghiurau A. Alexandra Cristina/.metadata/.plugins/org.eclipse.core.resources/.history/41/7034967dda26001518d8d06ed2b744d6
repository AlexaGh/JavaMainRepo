
public class Fibonacci {
	public static void main(String[] args) {
		
		long sum = 0;
		long f0 = 0;
		long f1 = 1;
		long fn = 0;
		
		for(int counter = 0; counter < 4000000; counter ++){
			fn = f0 + f1;
			if(f1 %2 == 0){
				sum += f1; 
			}
			f0 = f1;
			f1 = fn;
		}
		System.out.println(sum);
	}
}
