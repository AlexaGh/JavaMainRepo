import java.util.Scanner;

public class Palindrome {

	public static void main(String[] args) {
		int n = 432156;

		if (checkIfPalindrome(n) == true) {
			System.out.println("palindrome");
		} else {
			System.out.println("not palindrome");
		}
		if (checkIfPalindrome(d) == true) {
			System.out.println("palindrome");
		} else {
			System.out.println("not palindrome");
		}
		if (checkIfPalindrome(f) == true) {
			System.out.println("palindrome");
		} else {
			System.out.println("not palindrome");
		}
		int product;
		for(int counter1 = 1; counter1 <= 3; counter1 ++){
			for(int counter2 = 3; counter2>=1; counter2--){
				product = counter1 * counter2;
				System.out.println(counter1+ " * "+ counter2 +" = "+ product);
			}
		}

	}

	public static boolean checkIfPalindrome(int n) {

		int length = (int) (Math.log10(n) + 1);
		if (length == 5) {
			if ((n / 10000 == n % 10)) {
				n /= 10;
				if (n % 10 == (n / 100) % 10) {
					return true;
				}
			}
		}
		if (length == 6) {
			if ((n / 100000 == n % 10)) {
				n /= 10;
				if (n % 10 == (n / 1000) % 10) {
					n /= 10;
					if (n % 10 == (n / 10) % 10)
						return true;
				}
			}
		}

		return false;
	}
}
